<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.teqiqwizard" name="[COLOR darkorange]TeQ IQ Build Wizard[/COLOR]" version="2.0.1" provider-name="Playonmonkey">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Build Installer</summary>
    <description lang="en">Build Installer Based on the Aftermath wizard</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.teqiqiptv"
       name="[COLOR darkorange]TeQ IQ IPTV[/COLOR]"
       version="1.2"
       provider-name="TeQ IQ">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">TeQ IQ IPTV</summary>
    <language>en</language>
    <description lang="en">TeQ IQ - Live TV / PPV / Sports / Kids / Movies / Entertainment at an affordable price for all.</description>
     <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.TeQ-IQ-repo" name="[COLOR darkorange]TeQ I.Q. Repo[/COLOR] Repository" version="1.1" provider-name="The King">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="TeQ I.Q.">
            <info compressed="false">https://raw.githubusercontent.com/TheKing-Kodi/TeQ-IQ-TV/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/TheKing-Kodi/TeQ-IQ-TV/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/TheKing-Kodi/TeQ-IQ-TV/master/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Install My Addons</summary>
            <description>Download and install addons from TeQ I.Q.</description>
            <platform>all</platform>
    </extension>
</addon>

